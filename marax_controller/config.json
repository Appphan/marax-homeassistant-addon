{
  "name": "MaraX Controller",
  "version": "2.5.7",
  "slug": "marax_controller",
  "description": "Web-based interface for managing MaraX ESP32 Controller profiles, monitoring brew stats, and controlling your espresso machine",
  "url": "https://github.com/Appphan/maraxcontroller_V2",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ingress": true,
  "ingress_port": 8080,
  "panel_icon": "mdi:coffee",
  "homeassistant": "2023.1.0",
  "ports": {
    "8080/tcp": null
  },
  "ports_description": {
    "8080/tcp": "Web interface port"
  },
  "options": {
    "mqtt_broker": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_base_topic": "marax",
    "update_interval": 1,
    "log_level": "info"
  },
  "schema": {
    "mqtt_broker": "str",
    "mqtt_port": "int",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_base_topic": "str",
    "update_interval": "int",
    "log_level": "list(debug|info|warning|error)"
  },
  "map": [
    "config:rw"
  ]
}

